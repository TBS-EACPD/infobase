    "value_type": "enums",
    "enum_values": {
      "one": {
        "en": "1",
        "fr": "1"
      },
      "two": {
        "en": "2",
        "fr": "2"
      },
      "three": {
        "en": "3",
        "fr": "3"
      },
      "four": {
        "en": "4",
        "fr": "4"
      },
      "five": {
        "en": "5",
        "fr": "5"
      }
    },
    "style": "horizontal"
  },

  "other_details": {
    "required": false,
    "value_type": "string",
