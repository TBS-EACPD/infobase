{
  "plugins": [
    "node"
  ],
  "extends": [ "../.eslintrc.json", "plugin:node/recommended" ],
  "env": {
    "node": true
  },
  "rules": {
      "node/no-unsupported-features": ["error", {"ignores": ["modules"]}]
  },
  "globals": {
    "_": true,
    "describe": true,
    "it": true,
    "expect": true,
    "process": true
  }
}
