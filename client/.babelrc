{
  "env":{
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": false,
            "modules": false
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
    "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "@babel/syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        [
          "@babel/plugin-proposal-decorators",
          { "decoratorsBeforeExport": false }
        ]
      ]
    }
  }
}
