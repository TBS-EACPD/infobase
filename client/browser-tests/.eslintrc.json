
{
  "plugins": [
    "node"
  ],
  "extends": [ "../../.eslintrc.json", "plugin:node/recommended" ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalDecorators": true
    },
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "node": true
  },
  "rules": {
      "node/no-unsupported-features/es-syntax": ["error", {"ignores": ["modules"]}],
      "no-console": 0,
      "node/no-extraneous-import": 0
  },
  "globals": {
    "process": true,
    "fixture": true,
    "test": true
  }
}