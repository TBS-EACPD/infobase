{
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": false }],
    ["@babel/plugin-transform-class-properties"]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": { "version": "3.18" }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ]
}
