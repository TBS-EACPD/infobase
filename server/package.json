{
  "name": "infobase-server",
  "private": true,
  "description": "",
  "main": "transpiled_build/index.js",
  "directories": {
    "lib": "src",
    "build": "transpilied_build"
  },
  "engines": {
    "node": "^9.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "d3-dsv": "^1.0.8",
    "dataloader": "^1.4.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^2.11.0",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
    "mongoose": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/node": "^7",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-modules-commonjs": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^4.18.1",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^24",
    "jest-cli": "^24"
  },
  "scripts": {
    "pretest": "(USE_TEST_DATA=true; npm run load_models)",
    "test": "jest --coverage --forceExit",
    "posttest": "npm run load_models",
    "preci_test": "(USE_TEST_DATA=true; npm run transpiled_load_models)",
    "ci_test": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "debug": "babel-node --inspect src/server.js",
    "start": "babel-node src/server.js",
    "transpiled_start": "babel-node transpiled_build/server.js",
    "debug-brk": "babel-node --inspect-brk src/server.js",
    "load_models": "babel-node src/models/populate_models.js",
    "transpiled_load_models": "babel-node transpiled_build/models/populate_models.js",
    "debug_load_models": "babel-node --inspect-brk src/models/populate_models.js",
    "prebuild": "rm -rf transpiled_build",
    "build": "babel src/ --out-dir transpiled_build --copy-files --ignore node_modules"
  },
