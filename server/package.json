{
  "name": "infobase-server",
  "private": true,
  "description": "",
  "main": "transpiled_build/index.js",
  "directories": {
    "lib": "src",
    "build": "transpilied_build"
  },
  "engines": {
    "node": "^9.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "d3-dsv": "^1.0.8",
    "dataloader": "^1.4.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^2.11.0",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
    "mongoose": "^5.5.8"
  },
  "devDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/node": "^7",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-modules-commonjs": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^4.18.1",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^24",
    "jest-cli": "^24",
    "kill-port": "^1.5.1",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "pretest": "(export USE_TEST_DATA='true' && babel-node src/populate_db.js)",
    "test": "jest --coverage --forceExit",
    "posttest": "babel-node src/populate_db.js",
    "preci_test": "(export USE_TEST_DATA='true' && npm run transpiled_populate_db)",
    "ci_test": "jest --coverage --forceExit",
    "test:watch": "jest --watch",
    "start": "nodemon --config nodemon_start_config.json --watch src --watch package-lock.json --exec babel-node src/server.js",
    "debug": "nodemon --config nodemon_start_config.json --watch src --watch package-lock.json --exec babel-node --inspect ./src/server.js",
    "debug-brk": "nodemon --watch src --watch package-lock.json --exec babel-node --inspect-brk ./src/server.js",
    "transpiled_start": "babel-node transpiled_build/server.js",
    "populate_db": "nodemon --watch ../data -e csv --exec babel-node src/populate_db.js",
    "populate_db_exitcrash": "nodemon --exitcrash --watch ../data -e csv --exec babel-node src/populate_db.js",
    "debug_populate_db": "nodemon --watch ../data -e csv --exec babel-node --inspect src/populate_db.js",
    "transpiled_populate_db": "babel-node transpiled_build/populate_db.js",
    "prebuild": "rm -rf transpiled_build",
    "build": "babel src/ --out-dir transpiled_build --copy-files --ignore node_modules"
  },
  "authors": [
    "Alex Cousineau-Leduc",
    "Stephen O'Neil"
  ],
  "license": "BSD-2-Clause"
}
